# Project configuration
PROJECT_NAME=test_amemcp

# LLM Backend configuration
# Supported options: openai, gemini, ollama, mock
# LLM_BACKEND=openai

# # OpenAI configuration
# # LLM model examples: gpt-4, gpt-3.5-turbo
# # Embedding model example: text-embedding-ada-002
# LLM_MODEL=gpt-4o
# EMBED_MODEL=text-embedding-3-large
# OPENAI_API_KEY=your_openai_api_key_here

# Google Gemini configuration
# Uncomment and fill these if using the gemini backend
LLM_BACKEND=gemini
LLM_MODEL=gemini-2.5-pro-exp-03-25
EMBED_MODEL=gemini-embedding-exp-03-07
GOOGLE_API_KEY=your_google_api_key_here

# Ollama configuration (local models)
# Uncomment and fill these if using the ollama backend
# LLM_BACKEND=ollama
# LLM_MODEL=llama3
# EMBED_MODEL=llama3

# Jina Reranker configuration
JINA_RERANKER_MODEL=jinaai/jina-reranker-v2-base-multilingual
# JINA_API_KEY=your_jina_api_key_here

# Server configuration
HOST=0.0.0.0
PORT=8000

# Logging configuration
LOG_LEVEL=INFO

# Additional configuration
PERSIST_DIRECTORY=./data/chroma_db
# COLLECTION_PREFIX=amem # Optional: Defaults to 'amem' in code

# LiteLLM logging - IMPORTANT: Set to "false" to prevent verbose logging
LITELLM_VERBOSE=false
